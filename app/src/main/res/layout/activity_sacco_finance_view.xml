<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#218FE5"
    tools:context=".saccoFinanceView">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar3"
        android:layout_width="493dp"
        android:layout_height="69dp"
        android:background="@color/white"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="779dp" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="400dp"
        android:layout_height="52dp"
        android:layout_marginTop="88dp"
        android:background="@color/white"
        android:hapticFeedbackEnabled="false"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="8dp" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="-59dp"
        tools:layout_editor_absoluteY="215dp" >

    </androidx.cardview.widget.CardView>

    <ImageButton
        android:id="@+id/imageButton21"
        android:layout_width="84dp"
        android:layout_height="53dp"
        android:background="#218FE5"
        app:srcCompat="@drawable/ic_menu"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="21dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="69dp"
        android:layout_height="58dp"
        app:srcCompat="@mipmap/i_logout"
        tools:layout_editor_absoluteX="396dp"
        tools:layout_editor_absoluteY="785dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="69dp"
        android:layout_height="58dp"
        app:srcCompat="@mipmap/i_message"
        tools:layout_editor_absoluteX="225dp"
        tools:layout_editor_absoluteY="779dp" />

    <ImageButton
        android:id="@+id/imageButton23"
        android:layout_width="35dp"
        android:layout_height="36dp"
        android:layout_marginStart="336dp"
        android:layout_marginLeft="336dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="40dp"
        android:background="#218FE5"
        app:layout_constraintBottom_toTopOf="@+id/toolbar"
        app:layout_constraintEnd_toEndOf="@+id/toolbar"
        app:layout_constraintHorizontal_bias="0.66"
        app:layout_constraintStart_toStartOf="@+id/toolbar"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_notifications_24" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="400dp"
        android:layout_height="208dp"
        android:background="#FFFFFF"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="451dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="394dp"
            android:layout_height="88dp"
            android:background="#FFFFFF"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="465dp">

            <ImageButton
                android:id="@+id/imageButton12"
                android:layout_width="126dp"
                android:layout_height="83dp"
                android:background="#218FE5"
                app:srcCompat="@mipmap/ic_finance"
                tools:layout_editor_absoluteX="134dp"
                tools:layout_editor_absoluteY="3dp" />

            <ImageButton
                android:id="@+id/imageButton9"
                android:layout_width="126dp"
                android:layout_height="83dp"
                android:layout_gravity="clip_vertical"
                android:background="#218FE5"
                android:baselineAlignBottom="false"
                android:cropToPadding="true"
                android:foregroundGravity="center"
                app:srcCompat="@mipmap/ic_transact"
                tools:layout_editor_absoluteX="6dp"
                tools:layout_editor_absoluteY="3dp" />

            <ImageButton
                android:id="@+id/imageButton18"
                android:layout_width="126dp"
                android:layout_height="83dp"
                android:background="#218FE5"
                app:srcCompat="@mipmap/ic_insure"
                tools:layout_editor_absoluteX="262dp"
                tools:layout_editor_absoluteY="3dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <ImageButton
            android:id="@+id/imageButton9"
            android:layout_width="120dp"
            android:layout_height="83dp"
            android:layout_gravity="clip_vertical"
            android:background="#218FE5"
            android:baselineAlignBottom="false"
            android:cropToPadding="true"
            android:foregroundGravity="center"
            app:srcCompat="@mipmap/ic_transact"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="16dp" />

        <ImageButton
            android:id="@+id/imageButton15"
            android:layout_width="120dp"
            android:layout_height="83dp"
            android:background="#218FE5"
            app:srcCompat="@mipmap/ic_loan"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="109dp" />

        <ImageButton
            android:id="@+id/imageButton11"
            android:layout_width="120dp"
            android:layout_height="83dp"
            android:background="#218FE5"
            app:srcCompat="@mipmap/ic_market"
            tools:layout_editor_absoluteX="272dp"
            tools:layout_editor_absoluteY="109dp" />

        <ImageButton
            android:id="@+id/imageButton17"
            android:layout_width="120dp"
            android:layout_height="83dp"
            android:background="#218FE5"
            app:srcCompat="@mipmap/ic_bank"
            tools:layout_editor_absoluteX="140dp"
            tools:layout_editor_absoluteY="109dp" />

        <ImageButton
            android:id="@+id/imageButton12"
            android:layout_width="120dp"
            android:layout_height="83dp"
            android:background="#218FE5"
            app:srcCompat="@mipmap/ic_finance"
            tools:layout_editor_absoluteX="140dp"
            tools:layout_editor_absoluteY="16dp" />

        <ImageButton
            android:id="@+id/imageButton18"
            android:layout_width="120dp"
            android:layout_height="83dp"
            android:background="#218FE5"
            app:srcCompat="@mipmap/ic_insure"
            tools:layout_editor_absoluteX="272dp"
            tools:layout_editor_absoluteY="15dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="400dp"
        android:layout_height="175dp"
        android:background="#FDFFFFFF"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="267dp">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Deposits"
            android:textColor="@color/black"
            android:textSize="20sp"
            tools:layout_editor_absoluteX="33dp"
            tools:layout_editor_absoluteY="21dp" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Withdraws"
            android:textColor="@color/black"
            android:textSize="20sp"
            tools:layout_editor_absoluteX="31dp"
            tools:layout_editor_absoluteY="75dp" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Active Loans"
            android:textColor="@color/black"
            android:textSize="20sp"
            tools:layout_editor_absoluteX="29dp"
            tools:layout_editor_absoluteY="123dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/button2"
        android:layout_width="132dp"
        android:layout_height="40dp"
        android:text="Withdraw"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:backgroundTint="@color/white"
        tools:layout_editor_absoluteX="232dp"
        tools:layout_editor_absoluteY="93dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="132dp"
        android:layout_height="40dp"
        android:text="Deposit"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:backgroundTint="@color/white"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="95dp" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="400dp"
        android:layout_height="108dp"
        android:background="#FFFFFF"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="150dp">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Lima Wallet"
            android:textColor="@color/black"
            android:textSize="24sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="24dp"
            tools:layout_editor_absoluteY="22dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>